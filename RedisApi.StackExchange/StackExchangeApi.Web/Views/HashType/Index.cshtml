@model Dictionary<string, string>   
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<hr />

<form enctype="application/x-www-form-urlencoded" method="post" asp-action="AddItem">
    <input type="text" name="field" />
    <input type="text" name="value" />
    <button type="submit" class="btn btn-outline-success">Add</button>
</form>

@foreach(var item in Model)
{
    <div>@item.Key => @item.Value <a asp-action="DeleteItem" asp-route-field="@item.Key" class="btn btn-danger">Sil</a></div>
}